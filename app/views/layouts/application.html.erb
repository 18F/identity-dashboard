    <!-- Gov banner END --> 
    <div class="usa-nav-container">
      <div class="usa-navbar">
        <button class="usa-menu-btn">Menu</button>
        <div class="usa-logo" id="logo">
          <em class="usa-logo-text">
            <a href="/" accesskey="1" title="Home" aria-label="login.gov dashboard">
              <img src="/assets/img/logo.svg" alt="login.gov logo"><span>Dashboard</span>
            </a>
          </em>
        </div>
      </div>
      <nav role="navigation" class="usa-nav">
        <button class="usa-nav-close">
          <img src="/assets/uswds/img/close.svg" alt="close">
        </button>
        <ul class="usa-nav-primary usa-accordion">
          <% if user_signed_in? %>
            <li>
              <a class="usa-nav-link" href="<%= service_providers_path %>">
                <span>Apps</span>
              </a>
            </li>
            <% if current_user.admin? %>
            <li>
              <button class="usa-accordion-button usa-nav-link" aria-expanded="false" aria-controls="side-nav-1">
                <span>Admin</span>
              </button>
              <ul id="side-nav-1" class="usa-nav-submenu">
                <li><a href="/service_providers/all">Apps</a></li>
                <li><a href="/users">Users</a></li>
                <li><a href="/groups">Groups</a></li>
                <li><a href="/emails">Emails</a></li>
              </ul>
            </li>
            <% end %>
          <li>
            <a class="usa-nav-link" href="<%= destroy_user_session_path %>">
              <span>Log out</span>
            </a>
          </li>
          <% else %>
          <li>
            <a class="usa-nav-link" href="/auth/logindotgov">
              <span>Log in</span>
            </a>
          </li>
          <% end %>
        </ul>
      </nav>
    </div>
  </header>
  <div class="usa-overlay"></div>

  <main class="usa-grid usa-section usa-content usa-layout-docs" id="main-content">
    <div class="usa-layout-docs-main_content">
      <%= render 'layouts/messages' %>
      <%= yield %>
    </div>
  </main>
