<div class="usa-display">New group</div>


<%= simple_form_for(@group,
    html: { autocomplete: 'off', role: 'form' },
    url: groups_path) do |form| %>
  <%= form.error_notification %>
  <%= render 'form', form: form %>
  <%= form.button :submit, 'Create' %>
<% end %>
