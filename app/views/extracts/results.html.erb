<h1>Config Extraction Results</h1>
<p class="usa-prose">Results file: /tmp/config_extract_<%= @extract.ticket %></p>

<% if @failures.present? %>
  <h2><%= "#{@failures.length} #{@extract.search_by == 'teams' ? 'teams' : 'issuers' }" %> missing</h2>
  <ul class="usa-list">
    <% @failures.each do |f| %>
      <li><%= f %></li>
    <% end %>
  </ul>
<% end %>

<h2><%= @successes.length %> ServiceProvider rows found</h2>
<ul class="usa-list font-family-mono">
  <% @successes.each do |s| %>
    <li>team <%= s.group_id %> | <%= s.issuer %></li>
  <% end %>
</ul>
