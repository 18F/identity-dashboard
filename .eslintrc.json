{
  "extends": "plugin:@18f/eslint-plugin-identity/recommended",
  "plugins": ["@18f/eslint-plugin-identity"],
  "env": {
    "browser": true
  },
  "globals": {
    "expect": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [
      "error",
      { "devDependencies": ["{spec,config}/**/*"] }
    ]
  }
}
